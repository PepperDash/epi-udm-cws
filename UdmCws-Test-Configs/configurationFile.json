            {
              "deviceKey": "dsp-mock",
              "deviceIndex": 8,
              "customLabel": "Audio Processor",
              "description": "Biamp Tesira audio DSP - active during calls and presentations"
            },
            {
              "deviceKey": "lighting-control",
              "deviceIndex": 9,
              "customLabel": "Lighting System",
              "description": "Lutron lighting control - adjusts scenes based on activity"
