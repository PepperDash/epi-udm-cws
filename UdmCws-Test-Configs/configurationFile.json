          "videoSourceOn": "HDMI 1",
          "audioSourceOn": "HDMI 1",
          "customProperty1": "4K Resolution",
          "customProperty2": "85 inches",
          "customProperty3": "Samsung QN85",
          "customProperty4": "Firmware: v2.4.1",
          "customProperty5": "Mounted - Center Wall"
        }
      },
      {
        "key": "display-left",
        "name": "Left Display (LG 65\")",
        "type": "mockdevice",
        "group": "display",
        "properties": {
