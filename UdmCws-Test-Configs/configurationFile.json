              { "propertyKey": "property3", "label": "Codec SIP Address", "mapping": { "deviceKey": "codec-mock", "propertyPath": "CustomProperty2" } },
              { "propertyKey": "property4", "label": "Codec License Status", "mapping": { "deviceKey": "codec-mock", "propertyPath": "CustomProperty5" } },
              { "propertyKey": "property5", "label": "Camera Preset", "mapping": { "deviceKey": "camera-main", "propertyPath": "CustomProperty1" } },
              { "propertyKey": "property6", "label": "Camera Tracking", "mapping": { "deviceKey": "camera-main", "propertyPath": "CustomProperty5" } },
              { "propertyKey": "property7", "label": "DSP Matrix Size", "mapping": { "deviceKey": "dsp-mock", "propertyPath": "CustomProperty1" } },
              { "propertyKey": "property8", "label": "DSP Echo Cancel", "mapping": { "deviceKey": "dsp-mock", "propertyPath": "CustomProperty3" } },
              { "propertyKey": "property9", "label": "Lighting Scene", "mapping": { "deviceKey": "lighting-control", "propertyPath": "CustomProperty1" } },
              { "propertyKey": "property10", "label": "Lighting Level", "mapping": { "deviceKey": "lighting-control", "propertyPath": "CustomProperty2" } },
