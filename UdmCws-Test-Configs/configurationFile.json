          "videoSourceOn": "DM Output 3",
          "audioSourceOn": "None",
          "customProperty1": "1080p Resolution",
          "customProperty2": "65 inches",
          "customProperty3": "LG 65UM7300",
          "customProperty4": "Firmware: v5.30.05",
          "customProperty5": "Mounted - Right Wall"
        }
      },
      {
        "key": "codec-mock",
        "name": "Video Codec (Cisco Webex)",
        "type": "mockdevice",
        "group": "videoCodec",
        "properties": {
          "activityDevices": ["call"],
          "videoSourceOn": "Camera 1",
          "audioSourceOn": "Ceiling Mics",
          "customProperty1": "SIP Registered",
          "customProperty2": "webex.room@company.com",
          "customProperty3": "Bandwidth: 4 Mbps",
          "customProperty4": "Firmware: ce11.2.1",
          "customProperty5": "License: Active"
        }
      },
      {
        "key": "camera-main",
        "name": "PTZ Camera (Cisco Precision 60)",
        "type": "mockdevice",
        "group": "camera",
        "properties": {
          "activityDevices": ["call"],
          "videoSourceOn": "Active",
          "audioSourceOn": "None",
          "customProperty1": "Preset: Speaker Track",
          "customProperty2": "Zoom: 1.5x",
          "customProperty3": "Pan: -15°",
          "customProperty4": "Tilt: 5°",
          "customProperty5": "Tracking: Enabled"
        }
      },
      {
        "key": "dsp-mock",
        "name": "Audio Processor (Biamp Tesira)",
        "type": "mockdevice",
        "group": "dsp",
        "properties": {
          "activityDevices": ["call", "present"],
          "videoSourceOn": "None",
          "audioSourceOn": "Mic Array + HDMI",
          "customProperty1": "Matrix: 8x8",
          "customProperty2": "Room Combine: Off",
          "customProperty3": "Echo Cancel: On",
          "customProperty4": "Firmware: v4.3.2",
          "customProperty5": "Preset: Presentation"
        }
      },
      {
        "key": "lighting-control",
        "name": "Lighting Controller (Lutron)",
        "type": "mockdevice",
        "group": "lighting",
        "properties": {
