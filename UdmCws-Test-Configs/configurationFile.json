            {
              "deviceKey": "av-switcher",
              "deviceIndex": 10,
              "customLabel": "AV Switcher",
              "description": "Crestron DM matrix switcher - routes sources based on activity"
            },
            {
              "deviceKey": "mic-system",
              "deviceIndex": 11,
              "customLabel": "Microphone Array",
              "description": "Shure ceiling microphone system - active during calls and presentations"
            },
            {
              "deviceKey": "doc-camera",
              "deviceIndex": 12,
              "customLabel": "Document Camera",
              "description": "Elmo document camera - available for presentations"
            },
            {
              "deviceKey": "projector",
              "deviceIndex": 13,
              "customLabel": "Ceiling Projector",
              "description": "Epson laser projector - powers on for presentations"
