{
  "template": {
    "info": {
      "comment": "UDM-CWS Demo Configuration - Realistic AV System with Activity-Based Control",
      "version": "2.24.0",
      "description": "Comprehensive demo environment featuring real devices (codec, DSP) and mock AV equipment with realistic custom properties. Mock devices respond to room state and activity changes.",
      "systemType": "custom",
      "processorType": "rmc4"
    },
    "devices": [
      {
        "key": "processor",
        "name": "Control Processor",
        "type": "rmc4",
        "group": "processor",
        "properties": {}
      },
      {
        "key": "codec-1",
        "name": "Main Room Codec",
        "type": "ciscoRoomOS",
        "group": "videoCodec",
        "properties": {
          "control": {
            "endOfLineString": "\n",
            "deviceReadyResponsePattern": "",
            "method": "Ssh",
            "tcpSshProperties": {
              "address": "10.11.50.135",
              "port": 22,
              "autoReconnect": true,
              "AutoReconnectIntervalMs": 10000,
              "username": "admin",
              "password": "cr3str0n"
            }
          },
          "phonebookDisableAutoPopulate": true,
          "phonebookMode": "Corporate",
          "showSelfViewByDefault": true,
          "sharing": {
            "autoShareContentWhileInCall": false,
            "defaultShareLocalOnly": true
          },
          "joinableCooldownSeconds": 0,
          "phonebookResultsLimit": 50,
          "defaultTrackingMode": "SpeakerTrack",
          "overrideMeetingsLimit": true,
          "cameraInfo": [
            {
              "CameraNumber": 1,
              "Name": "Main Camera"
            }
          ]
        }
      },
      {
        "key": "dsp-1",
        "name": "Room DSP",
        "type": "tesiraDsp",
        "group": "dsp",
        "properties": {
          "control": {
            "endOfLineString": "\n",
            "deviceReadyResponsePattern": "",
