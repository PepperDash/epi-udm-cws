          "videoSourceOn": "DM Output 2",
          "audioSourceOn": "None",
          "customProperty1": "1080p Resolution",
          "customProperty2": "65 inches",
          "customProperty3": "LG 65UM7300",
          "customProperty4": "Firmware: v5.30.05",
          "customProperty5": "Mounted - Left Wall"
        }
      },
      {
        "key": "display-right",
        "name": "Right Display (LG 65\")",
        "type": "mockdevice",
        "group": "display",
        "properties": {
