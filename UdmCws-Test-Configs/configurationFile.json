            "tcpSshProperties": {
              "address": "10.11.50.192",
              "port": 22,
              "autoReconnect": true,
              "AutoReconnectIntervalMs": 10000,
              "username": "default",
              "password": "",
              "disableSshEcho": true
            }
          }
        }
      },
      {
        "key": "occupancy-1",
        "name": "Crestron Occupancy Sensor (CEN-ODT-C-POE)",
        "type": "cenodtcpoe",
        "group": "occupancy",
        "properties": {
          "control": {
            "method": "ipid",
            "ipid": "0x07",
            "tcpSshProperties": {
              "address": "10.11.50.51",
              "port": 41794
            }
          }
        }
      },
      {
        "key": "mock-occupancy-1",
        "name": "Mock Occupancy Sensor (Room State)",
