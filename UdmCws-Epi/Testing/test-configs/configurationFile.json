{
  "template": {
    "info": {
      "comment": "PepperDash Essentials Test Configuration with UDM-CWS Plugin",
      "version": "2.24.0",
      "description": "Test environment with Cisco codec, Biamp DSP, and UDM-CWS monitoring. UDM-CWS creates its own HTTP server - no EssentialsWebApi needed.",
      "systemType": "custom",
      "processorType": "rmc4"
    },
    "devices": [
      {
        "key": "processor",
        "name": "Control Processor",
        "type": "processor",
        "group": "processor",
        "properties": {}
      },
      {
        "key": "codec-1",
        "name": "Main Room Codec",
        "type": "ciscoRoomOS",
        "group": "videoCodec",
        "properties": {
          "control": {
            "endOfLineString": "\n",
            "deviceReadyResponsePattern": "",
            "method": "Ssh",
            "tcpSshProperties": {
              "address": "10.11.50.135",
              "port": 22,
              "autoReconnect": true,
              "AutoReconnectIntervalMs": 10000,
              "username": "admin",
              "password": "cr3str0n"
            }
          },
          "phonebookDisableAutoPopulate": true,
          "phonebookMode": "Corporate",
          "showSelfViewByDefault": true,
          "sharing": {
            "autoShareContentWhileInCall": false,
            "defaultShareLocalOnly": true
          },
          "joinableCooldownSeconds": 0,
          "phonebookResultsLimit": 50,
          "defaultTrackingMode": "SpeakerTrack",
          "overrideMeetingsLimit": true,
          "cameraInfo": [
            {
              "CameraNumber": 1,
              "Name": "Main Camera"
            }
          ]
        }
      },
      {
        "key": "dsp-1",
        "name": "Room DSP",
        "type": "tesiraDsp",
        "group": "dsp",
        "properties": {
          "control": {
            "endOfLineString": "\n",
            "deviceReadyResponsePattern": "",
            "method": "ssh",
            "tcpSshProperties": {
              "address": "10.11.50.192",
              "port": 22,
              "autoReconnect": true,
              "AutoReconnectIntervalMs": 10000,
              "username": "default",
              "password": "",
              "disableSshEcho": true
            }
          }
        }
      },
      {
        "key": "udmCws",
        "name": "UDM-CWS Room Monitoring",
        "type": "udmcws",
        "group": "plugin",
        "properties": {
          "apiVersion": "1.0.0",
          "feedbackMode": "deferred",
          "deviceMappings": [
            {
              "deviceKey": "codec-1",
              "deviceIndex": 1,
              "customLabel": "Video Codec",
              "description": "Main room video codec for conferencing"
            },
            {
              "deviceKey": "dsp-1",
              "deviceIndex": 2,
              "customLabel": "Room Audio DSP",
              "description": "Biamp Tesira audio processor"
            }
          ],
          "standardProperties": {
            "version": "1.0.0",
            "roomDeviceKey": "room-1"
          }
        }
      }
    ],
    "rooms": [
      {
        "key": "room-1",
        "name": "Conference Room 1",
        "type": "standardRoom",
        "properties": {
          "videoCodecKey": "codec-1",
          "defaultSourceItem": "codec-1",
          "sourceListKey": "room-1",
          "destinationListKey": "room-1"
        }
      }
    ],
    "sourceLists": {
      "room-1": {
        "codec-1": {
          "sourceKey": "codec-1",
          "type": "route",
          "name": "Cisco Codec",
          "order": 1,
          "includeInSourceList": true
        }
      }
    },
    "destinationLists": {
      "room-1": {}
    }
  },
  "system": {}
}
