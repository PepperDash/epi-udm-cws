{
  "template": {
    "info": {
      "comment": "Comprehensive UDM-CWS Test Configuration - Real + Mock Devices",
      "version": "2.24.0",
      "description": "Complete test environment with real devices (codec, DSP, Crestron occupancy) AND mock plugins. Swap deviceKeys in UDM-CWS config to test different combinations.",
      "systemType": "custom",
      "processorType": "rmc4"
    },
    "devices": [
      {
        "key": "processor",
        "name": "Control Processor",
        "type": "rmc4",
        "group": "processor",
        "properties": {}
      },
      {
        "key": "codec-1",
        "name": "Main Room Codec",
        "type": "ciscoRoomOS",
        "group": "videoCodec",
        "properties": {
          "control": {
            "endOfLineString": "\n",
            "deviceReadyResponsePattern": "",
            "method": "Ssh",
            "tcpSshProperties": {
              "address": "10.11.50.135",
              "port": 22,
              "autoReconnect": true,
              "AutoReconnectIntervalMs": 10000,
              "username": "admin",
              "password": "cr3str0n"
            }
          },
          "phonebookDisableAutoPopulate": true,
          "phonebookMode": "Corporate",
          "showSelfViewByDefault": true,
          "sharing": {
            "autoShareContentWhileInCall": false,
            "defaultShareLocalOnly": true
          },
          "joinableCooldownSeconds": 0,
          "phonebookResultsLimit": 50,
          "defaultTrackingMode": "SpeakerTrack",
          "overrideMeetingsLimit": true,
          "cameraInfo": [
            {
              "CameraNumber": 1,
              "Name": "Main Camera"
            }
          ]
        }
      },
      {
        "key": "dsp-1",
        "name": "Room DSP",
        "type": "tesiraDsp",
        "group": "dsp",
        "properties": {
          "control": {
            "endOfLineString": "\n",
            "deviceReadyResponsePattern": "",
            "method": "ssh",
            "tcpSshProperties": {
              "address": "10.11.50.192",
              "port": 22,
              "autoReconnect": true,
              "AutoReconnectIntervalMs": 10000,
              "username": "default",
              "password": "",
              "disableSshEcho": true
            }
          }
        }
      },
      {
        "key": "occupancy-1",
        "name": "Crestron Occupancy Sensor (CEN-ODT-C-POE)",
        "type": "cenodtcpoe",
        "group": "occupancy",
        "properties": {
          "control": {
            "method": "ipid",
            "ipid": "0x07",
            "tcpSshProperties": {
              "address": "10.11.50.51",
              "port": 41794
            }
          }
        }
      },
      {
        "key": "mock-occupancy-1",
        "name": "Mock Occupancy Sensor (Auto-Toggle)",
        "type": "mockoccupancy",
        "group": "occupancy",
        "properties": {}
      },
      {
        "key": "mock-device-1",
        "name": "Mock Display (Full Features)",
        "type": "mockdevice",
        "group": "display",
        "properties": {}
      },
      {
        "key": "mock-device-2",
        "name": "Mock Codec (Full Features)",
        "type": "mockdevice",
        "group": "codec",
        "properties": {}
      },
      {
        "key": "udmCws",
        "name": "UDM-CWS Room Monitoring",
        "type": "udmcws",
        "group": "plugin",
        "properties": {
          "apiVersion": "1.0.0",
          "feedbackMode": "deferred",
          "deviceMappings": [
            {
              "deviceKey": "codec-1",
              "deviceIndex": 1,
              "customLabel": "Video Codec",
              "description": "Main room video codec for conferencing"
            },
            {
              "deviceKey": "dsp-1",
              "deviceIndex": 2,
              "customLabel": "Room Audio DSP",
              "description": "Biamp Tesira audio processor"
            },
            {
              "deviceKey": "mock-device-1",
              "deviceIndex": 3,
              "customLabel": "Mock Display",
              "description": "Test display with power/warming/cooling/usage"
            },
            {
              "deviceKey": "mock-device-2",
              "deviceIndex": 4,
              "customLabel": "Mock Codec",
              "description": "Test codec with all UDM-CWS features"
            }
          ],
          "standardProperties": {
            "version": "1.0.0",
            "roomDeviceKey": "mock-room-1",
            "occupancyDeviceKey": "occupancy-1",
            "activityMapping": {
              "getProperty": {
                "deviceKey": "mock-room-1",
                "propertyPath": "Activity"
              }
            },
            "helpRequestMapping": {
              "deviceKey": "mock-room-1",
              "propertyPath": "HelpRequest"
            },
            "customPropertyMappings": [
              { "propertyKey": "property1", "label": "Codec In Call", "mapping": { "deviceKey": "codec-1", "propertyPath": "IsInCall" } },
              { "propertyKey": "property2", "label": "Room Occupied", "mapping": { "deviceKey": "occupancy-1", "propertyPath": "RoomIsOccupiedFeedback.BoolValue" } },
              { "propertyKey": "property3", "label": "Property 3", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property3" } },
              { "propertyKey": "property4", "label": "Property 4", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property4" } },
              { "propertyKey": "property5", "label": "Property 5", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property5" } },
              { "propertyKey": "property6", "label": "Property 6", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property6" } },
              { "propertyKey": "property7", "label": "Property 7", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property7" } },
              { "propertyKey": "property8", "label": "Property 8", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property8" } },
              { "propertyKey": "property9", "label": "Property 9", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property9" } },
              { "propertyKey": "property10", "label": "Property 10", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property10" } },
              { "propertyKey": "property11", "label": "Property 11", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property11" } },
              { "propertyKey": "property12", "label": "Property 12", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property12" } },
              { "propertyKey": "property13", "label": "Property 13", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property13" } },
              { "propertyKey": "property14", "label": "Property 14", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property14" } },
              { "propertyKey": "property15", "label": "Property 15", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property15" } },
              { "propertyKey": "property16", "label": "Property 16", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property16" } },
              { "propertyKey": "property17", "label": "Property 17", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property17" } },
              { "propertyKey": "property18", "label": "Property 18", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property18" } },
              { "propertyKey": "property19", "label": "Property 19", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property19" } },
              { "propertyKey": "property20", "label": "Property 20", "mapping": { "deviceKey": "mock-room-1", "propertyPath": "Property20" } }
            ]
          }
        }
      }
    ],
    "rooms": [
      {
        "key": "mock-room-1",
        "name": "Mock Conference Room (20 Custom Properties)",
        "type": "mockroom",
        "properties": {
          "helpRequestMessage": "Support needed in Conference Room 1",
          "defaultActivity": "idle",
          "deviceKeys": ["mock-device-1", "mock-device-2"],
          "property1": "Custom Value 1",
          "property2": "Custom Value 2",
          "property3": "Custom Value 3",
          "property4": "Custom Value 4",
          "property5": "Custom Value 5",
          "property6": "Custom Value 6",
          "property7": "Custom Value 7",
          "property8": "Custom Value 8",
          "property9": "Custom Value 9",
          "property10": "Custom Value 10",
          "property11": "Custom Value 11",
          "property12": "Custom Value 12",
          "property13": "Custom Value 13",
          "property14": "Custom Value 14",
          "property15": "Custom Value 15",
          "property16": "Custom Value 16",
          "property17": "Custom Value 17",
          "property18": "Custom Value 18",
          "property19": "Custom Value 19",
          "property20": "Custom Value 20"
        }
      }
    ],
    "sourceLists": {
      "room-1": {
        "codec-1": {
          "sourceKey": "codec-1",
          "type": "route",
          "name": "Cisco Codec",
          "order": 1,
          "includeInSourceList": true
        }
      }
    },
    "destinationLists": {
      "room-1": {}
    },
    "audioControlPointLists": {
      "room-1": {
        "levelControls": {},
        "presets": {}
      }
    }
  },
  "system": {},
  "_comments": {
    "testing_scenarios": {
      "scenario_1_all_real_devices": {
        "description": "Test with real codec, DSP, and Crestron occupancy sensor",
        "udmCws_standardProperties_occupancyDeviceKey": "occupancy-1",
        "udmCws_standardProperties_roomDeviceKey": "room-1"
      },
      "scenario_2_mock_occupancy": {
        "description": "Test with mock occupancy (auto-toggles every 30 seconds)",
        "udmCws_standardProperties_occupancyDeviceKey": "mock-occupancy-1",
        "note": "Change occupancyDeviceKey to 'mock-occupancy-1' in udmCws config"
      },
      "scenario_3_mock_room": {
        "description": "Test with mock room (20 custom properties)",
        "udmCws_standardProperties_roomDeviceKey": "mock-room-1",
        "note": "Change roomDeviceKey to 'mock-room-1' in udmCws config"
      },
      "scenario_4_all_mock": {
        "description": "Test with all mock devices",
        "udmCws_deviceMappings": "Use only mock-device-1, mock-device-2",
        "udmCws_standardProperties_occupancyDeviceKey": "mock-occupancy-1",
        "udmCws_standardProperties_roomDeviceKey": "mock-room-1"
      }
    },
    "device_types": {
      "ciscoCodec": "Update with actual Cisco codec type from your Essentials plugins",
      "biampTesira": "Update with actual Biamp type from your Essentials plugins",
      "cenodtcpoe": "Crestron CEN-ODT-C-POE occupancy sensor (built-in to Essentials)",
      "mockoccupancy": "Mock occupancy sensor - auto-toggles every 30 seconds",
      "mockdevice": "Mock device with power/warming/cooling/usage/routing",
      "mockroom": "Mock room with 20 custom properties and help request",
      "kpmgRoom": "Your custom room plugin"
    },
    "quick_test_steps": [
      "1. Deploy all packages (UdmCws-Epi.cplz + all 3 mock plugin .cplz files)",
      "2. Use this config as-is to test mix of real and mock devices",
      "3. Access: http://<processor-ip>/app01/udmcws/roomstatus",
      "4. Verify device1-4 show up with proper status",
      "5. Test PATCH: {\"standard\": {\"state\": \"off\"}} to shutdown room",
      "6. Test PATCH: {\"standard\": {\"state\": \"on\"}} to power on room",
      "7. Watch mock devices warm/cool (2 second delays)",
      "8. Watch mock occupancy toggle every 30 seconds",
      "9. Switch occupancyDeviceKey to test Crestron vs Mock occupancy"
    ]
  }
}
